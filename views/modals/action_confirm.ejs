<!-- Confirm Modal -->
<div class="modal fade"
     id="systemConfirmModal"
     tabindex="-1"
     role="dialog"
     aria-labelledby="systemConfirmModalLabel"
     aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button"
                        class="close"
                        data-dismiss="modal"
                        aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="systemConfirmModalLabel">Modal title</h4>
            </div>
            <div class="modal-body"></div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default btn-simple" data-dismiss="modal">No</button>
                <button type="button" class="btn btn-info btn-simple">Yes</button>
            </div>
        </div>
    </div>
</div>
